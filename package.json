{
  "name": "expo-react-native-boilerplate",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "jest",
    "start": "expo start",
    "test:ci": "jest --ci",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "android": "expo start --android",
    "test:coverage": "jest --coverage",
    "build:storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test:storybook": "npx test-storybook",
    "start:client": "expo start --dev-client",
    "commitlint": "npx --no -- commitlint --edit",
    "lint": "eslint ./src/**/*.tsx ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.tsx ./src/**/*.ts --fix",
    "prettier": "prettier --check ./src/**/*.tsx ./src/**/*.ts --fix",
    "prettier:fix": "prettier --write ./src/**/*.tsx ./src/**/*.ts --fix",
    "build:chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "build:ios:development": "eas build --profile development --platform ios",
    "build:android:development": "eas build --profile development --platform android",
    "build:ios:development:simulator": "eas build --profile development-simulator --platform ios"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "0.2.3",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@reduxjs/toolkit": "2.0.1",
    "@sentry/react-native": "5.5.0",
    "@types/react": "18.2.47",
    "expo": "50.0.0-preview.9",
    "expo-application": "5.8.1",
    "expo-build-properties": "0.11.0",
    "expo-constants": "15.4.2",
    "expo-contacts": "12.8.2",
    "expo-device": "5.9.1",
    "expo-font": "11.10.0",
    "expo-localization": "14.8.1",
    "expo-network": "5.8.0",
    "expo-splash-screen": "0.26.1",
    "expo-status-bar": "1.11.1",
    "i18next": "23.7.16",
    "nativewind": "4.0.22",
    "react": "18.2.0",
    "react-i18next": "14.0.0",
    "react-native": "0.73.2",
    "react-native-reanimated": "3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "3.29.0",
    "react-native-web": "0.19.10",
    "react-redux": "9.0.4",
    "sentry-expo": "7.1.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@commitlint/types": "18.4.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.2.6",
    "@semantic-release/npm": "11.0.2",
    "@semantic-release/release-notes-generator": "12.1.0",
    "@storybook/addon-a11y": "7.6.7",
    "@storybook/addon-coverage": "1.0.0",
    "@storybook/addon-designs": "7.0.7",
    "@storybook/addon-essentials": "7.6.7",
    "@storybook/addon-interactions": "7.6.7",
    "@storybook/addon-links": "7.6.7",
    "@storybook/addon-react-native-web": "0.0.22",
    "@storybook/addon-storysource": "7.6.7",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "7.6.7",
    "@storybook/react": "7.6.7",
    "@storybook/react-webpack5": "7.6.7",
    "@storybook/test-runner": "0.16.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.4.3",
    "@types/expo": "33.0.1",
    "@types/jest": "29.5.11",
    "@ventsislavnikolov/eslint-config": "1.0.3",
    "babel-plugin-module-resolver": "5.0.0",
    "chromatic": "10.2.0",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-expo": "50.0.1",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "semantic-release": "22.0.12",
    "semantic-release-ado": "1.4.0",
    "storybook": "7.6.7",
    "storybook-dark-mode": "3.0.3",
    "ts-jest": "29.1.1"
  },
  "resolutions": {
    "@expo/config-plugins": "7.8.0",
    "@expo/metro-config": "0.16.0",
    "@expo/prebuild-config": "6.7.0",
    "expo-modules-autolinking": "1.9.0"
  }
}
