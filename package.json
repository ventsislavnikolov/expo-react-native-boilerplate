{
  "name": "expo-react-native-boilerplate",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky",
    "dev": "expo start",
    "start": "expo start",
    "test:ci": "jest --ci",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test:watch": "jest --watch",
    "lint:staged": "pnpx lint-staged",
    "android": "expo start --android",
    "test:coverage": "jest --coverage",
    "build:storybook": "storybook build",
    "test:storybook": "npx test-storybook",
    "start:client": "expo start --dev-client",
    "env:pull": "npx dotenv-vault@latest pull",
    "env:push": "npx dotenv-vault@latest push",
    "start:storybook": "storybook dev -p 6006",
    "lint:commit": "npx --no -- commitlint --edit",
    "format": "prettier --check ./src/**/*.{ts,tsx,json}",
    "format:write": "prettier --write ./src/**/*.{ts,tsx,json}",
    "build:ios:development": "eas build --profile development --platform ios",
    "build:chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "build:android:development": "eas build --profile development --platform android",
    "build:ios:development:simulator": "eas build --profile development-simulator --platform ios",
    "lint": "npx oxlint --jest-plugin --jsx-a11y-plugin -D all -A nursery -c ./.eslintrc.json --deny-warnings"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "0.2.3",
    "@expo/metro-runtime": "3.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/bottom-tabs": "6.5.20",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@reduxjs/toolkit": "2.2.5",
    "@sentry/react-native": "5.22.0",
    "expo": "51.0.8",
    "expo-application": "5.9.1",
    "expo-asset": "10.0.6",
    "expo-build-properties": "0.12.1",
    "expo-constants": "16.0.1",
    "expo-contacts": "13.0.3",
    "expo-dev-client": "4.0.14",
    "expo-device": "6.0.2",
    "expo-font": "12.0.5",
    "expo-localization": "15.0.3",
    "expo-network": "6.0.1",
    "expo-splash-screen": "0.27.4",
    "expo-status-bar": "1.12.1",
    "expo-updates": "0.25.14",
    "i18next": "23.11.4",
    "nativewind": "4.0.36",
    "react": "18.2.0",
    "react-i18next": "14.1.1",
    "react-native": "0.74.1",
    "react-native-reanimated": "3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-web": "0.19.10",
    "react-redux": "9.1.2",
    "tailwindcss": "3.4.3"
