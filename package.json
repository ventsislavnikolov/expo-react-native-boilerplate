    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.0.4",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "13.0.0",
    "@storybook/addon-a11y": "8.1.1",
    "@storybook/addon-coverage": "1.0.3",
    "@storybook/addon-designs": "8.0.1",
    "@storybook/addon-essentials": "8.1.1",
    "@storybook/addon-interactions": "8.1.1",
    "@storybook/addon-links": "8.1.1",
    "@storybook/addon-react-native-web": "0.0.23",
    "@storybook/addon-storysource": "8.1.1",
    "@storybook/addon-themes": "8.1.1",
    "@storybook/blocks": "8.1.1",
    "@storybook/react": "8.1.1",
    "@storybook/react-webpack5": "8.1.1",
    "@storybook/test": "8.1.1",
    "@storybook/test-runner": "0.18.0",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.5.0",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.79",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "babel-loader": "9.1.3",
    "chromatic": "11.5.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
