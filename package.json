  "scripts": {
    "test": "jest",
    "dev": "expo start",
    "test:ci": "jest --ci",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "test:watch": "jest --watch",
    "android": "expo start --android",
    "test:coverage": "jest --coverage",
    "build:storybook": "storybook build",
    "test:storybook": "npx test-storybook",
    "dev:client": "expo start --dev-client",
    "start:storybook": "storybook dev -p 6006",
    "analyze": "EXPO_WEB_DEBUG=true npx expo export:web",
    "eslint:check": "eslint --ext .jsx,.js,.tsx,.ts src/",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "build:chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "build:ios:development": "eas build --profile development --platform ios",
    "build:android:development": "eas build --profile development --platform android",
    "build:ios:development:simulator": "eas build --profile development-simulator --platform ios",
    "coupling-graph": "npx madge --extensions js,jsx,css,md,mdx ./ --exclude 'release.config.js|README.md|prettier.config.js|coverage||yarn.lock|lint-staged.config.js|tsconfig.json|commitlint.config.js|web-build|.husky|.github|node_modules|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
