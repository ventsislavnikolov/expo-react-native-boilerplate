{
  "build": {
    "base": {
      "android": {
        "image": "ubuntu-22.04-jdk-17-ndk-r21e"
      },
      "env": {
        "EAS_PROJECT_ID": "65bfd5f8-08a5-4fc1-8938-097639c34225",
        "EXPO_PUBLIC_API_URL": "https://api.example.com"
      },
      "ios": {
        "bundler": "2.2.4",
        "image": "macos-ventura-13.6-xcode-15.1"
      },
      "node": "22.5.1",
      "yarn": "1.22.21"
    },
    "development": {
      "channel": "staging",
      "developmentClient": true,
      "distribution": "internal",
      "extends": "base"
    },
    "development-simulator": {
      "channel": "staging",
      "developmentClient": true,
      "distribution": "internal",
      "extends": "base",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "extends": "base"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "distribution": "store",
      "extends": "base"
    }
  },
  "cli": {
    "appVersionSource": "remote",
    "version": ">=0.54.1"
  },
  "submit": {
    "production": {}
  }
}
